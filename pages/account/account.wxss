.address{
  width:750rpx;
  height:140rpx;
  padding:30rpx 0;
  background-image:url(http://h5.xianzaishi.com//upload_48524d69b50ac5851188776db0080a29.jpg);
  background-repeat: no-repeat;
  background-size:cover;
  border-top:15rpx solid #eee;
  position:relative;
}
.address-p{
  font-size:28rpx;
  white-space: nowrap;
  line-height:50rpx;
  width:700rpx;
  margin:15rpx auto;
}
.address-span{
  display:inline-block;
  margin-right:30rpx;
}
.address-empty{
  line-height:140rpx;
  font-size:28rpx;
  text-align:center;
}
.arrow{
  width:27rpx;
  height:51rpx;
  position:absolute;
  top:50%;
  right:20rpx;
  margin-top:-25rpx;
  background-image:url(http://h5.xianzaishi.com//upload_dd41ecb9df9dbd56a41e9275364c4231.jpeg);
  background-size:cover;
  background-repeat:no-repeat;
}
.container{
  background-color:#eee;
}
.goods-tip{
  background-color:#fafbd9;
  font-size:28rpx;
  line-height:2.5em;
  color:#707056;
  padding:0 25rpx;
}
.goods-tip-header{
  color:#70aa52;
  margin-right:15rpx;
}
.goods{
  border-top:15rpx solid #eee;
  background-color: #fff;
}
.goods-list{
  width:750rpx;
  overflow-x:scroll;
  white-space: nowrap;
}
.goods-item{
  vertical-align: middle;
  width:650rpx;
  height:160rpx;
  position:relative;
  border-top:1rpx solid #fff;
  display: inline-block;
  border-left:1px solid #eee;

}
.goods-item image{
  width:120rpx;
  height:120rpx;
  margin:20rpx;
  float:left;
}
.goods-item-name{
  font-size:28rpx;
  margin-top:20rpx;
  line-height:40rpx;
  display:block;
  height: 80rpx;
  overflow:hidden;
  white-space: normal;
}
.goods-item-price{
  font-size:28rpx;
  display:block;
  margin-top:10rpx;
  line-height:40rpx;
  color:#f75218;
}
.goods-item-count{
  font-size:28rpx;
  position:absolute;
  right:20rpx;
  bottom:20rpx;  
  color:#c5c5c5;
}
.arrive-time{
  border-top:1px solid #eee;
  margin-top:15rpx;
  background-color:#fff;
}
.arrive-line{
  height:90rpx;
  position:relative;
  border-bottom:1px solid #eee;
}
.arrive-left{
  display:inline-block;
  margin-left:25rpx;  
  width:350rpx;
  font-size:28rpx;
  vertical-align: middle;
  line-height:90rpx;
}
.arrive-right{
  display:inline-block;
  width:310rpx;
  margin-right:65rpx;
  font-size:28rpx;
  vertical-align: middle;
  text-align:right;
  line-height:90rpx;
  color:#5a5a5a;
}
.arrive-view{
  line-height: 95rpx;
}
.arrive-coupon{
  width:50rpx;
  height:50rpx;
  border-radius:50%;
  background-color:#f75218;
  line-height: 55rpx;
  text-align:center;
  color:#fff;
  display: inline-block;
  vertical-align: middle;
}
.account-memo{
  display: block;
  width:700rpx;
  height:90rpx;
  line-height:90rpx;
  font-size:28rpx;
  margin:0 auto;
}
.arrive-line__bottom{
  border-bottom:none;
}
.account-weixin{
  background-image:url(data:image/jpeg;base64,);
  background-repeat:no-repeat;
  background-size:cover;
  width:50rpx;
  height:50rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right:15rpx;
}
.account-gou{
  background-image:url(data:image/jpeg;base64,);
  background-repeat:no-repeat;
  background-size:cover;
  width:50rpx;
  height:50rpx;
  display: inline-block;
  vertical-align: middle;
}
.arrive-right__bottom{
  margin-right:25rpx;
  width:350rpx;
}
.account-bottom{
  position:fixed;
  left:0;
  bottom:0;
  width:100%;
  background-color:#000;
  height:115rpx;
}
.account-bottom__place{
  height: 115rpx;
  margin-bottom:10rpx;
}
.account-bottom-total{
  line-height:115rpx;
}
.account-bottom-total text{
  font-size:32rpx;
  color:#fff;
}
.account-bottom-total text:first-child{
  margin-left:25rpx;
}
.account-bottom-btn{
  position:absolute;
  height:115rpx;
  line-height:115rpx;
  text-align: center;
  width:200rpx;
  background-color: #78b25d;
  color:#fff;
  right:0;
  top:0;
  font-size:32rpx;
}