<!--pages/pay-suc/pay-suc.wxml-->
<view class="container">
  <view class="header">
    <image  src="data:image/jpeg;base64,"/>
    <text>支付成功</text>
  </view>
  <view class="body">
    <view class="line">
      <text>付款金额 : </text>
      <text class="price">{{price}}</text><text>元</text>
    </view>
    <view class="line">
      <text>下单时间 : </text>
      <text>{{gmtCreate}}</text>
    </view>
  </view>
  <view class="foot">
    <navigator open-type="navigate" url="../mine-order/mine-order">
      <text>查看订单</text>
    </navigator>
    <navigator open-type="reLaunch" url="../index/index">
      <text>返回首页</text>
    </navigator>
  </view>
</view>