@import "../../component/pit-1/pit-1.wxss";

.container{
  background-color:#eee;
}
.header{
  background-color:#fff;
  border-bottom:1px solid #ddd;
  border-top:1px solid #fff;
  position:fixed;
  left:0;
  top:0;
  width:100%;
  z-index:1;
}
.header-place{
  height:105rpx;
}
.header-input{
  height:75rpx;
  line-height:85rpx;
  font-size:28rpx;
  background-color:#eee;
  display: inline-block;
  vertical-align: middle;
  margin:15rpx 0 15rpx 20rpx;
  border-radius:15rpx;
  width:520rpx; 
  padding-left:75rpx;
}
.header-icon{
  position:absolute;
  left:35rpx;
  top:30rpx;
}
.header-cart{
  width:75rpx;
  height:75rpx;
  position:absolute;
  right: 35rpx;
  top:15rpx;
  color:#70aa52;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}
.header-cart text{
  position:absolute;
  width:40rpx;
  height:40rpx;
  line-height:45rpx;
  text-align:center;
  background-color:#70aa52;
  color:#fff;
  font-size:25rpx;
  border-radius:50%;
  right:0;
  top:0;
}
.body{
  background-color:#fff;
  border-top:1rpx solid #fff;
}
.body-empty{
  text-align:center;
  font-size:35rpx;
  margin-top:250rpx;
}
.body-empty .body-empty-keyword{
  color:#70aa52;
}