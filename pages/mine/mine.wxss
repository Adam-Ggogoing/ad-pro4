.header{
  background-image:url(http://h5.xianzaishi.com//upload_53269aba30d8fc33b3ac18d06a618c67.png);
  width:750rpx;
  height:334rpx;
  background-repeat:none;
  background-size:cover;
  background-position:center;
  position:relative
}
.header-cover{
  position:absolute;
  width:100%;
  height:100%;
  left:0;
  top:0;
  background-color:rgba(0,0,0,.6);
  z-index:1;
}
.header-container__nologin{
  position:relative;
  z-index:2;
  padding-top:1px;
}

.header-logo{
  width:127rpx;
  height:127rpx;
  display: block;
  margin:60rpx auto 0;
}
.header-text{
  width:200rpx;
  height:60rpx;
  line-height:60rpx;
  background-color:#70aa52;
  color:#fff;
  text-align:center;
  display:block;
  border-radius:30rpx;
  font-size:30rpx;
  letter-spacing: 5rpx;
  text-indent: 5rpx;
  margin:0 auto;
}
.header-container__login{
  position:relative;
  z-index:2;
  padding-top:100rpx;
  margin-left:40rpx;
  display: inline-block;
  vertical-align: middle;
}
.header-logo__login{
  width:140rpx;
  height: 140rpx;
  display: inline-block;
  vertical-align:middle;
}
.header-info{
  display: inline-block;
  vertical-align: middle;
}
.header-phone{
  display:block;
  color:#fff;
  font-size:15px;
  margin-left:20rpx;
  height:50rpx;
  line-height: 50rpx;
}
.header-credit{
  display:block;
  margin-left:20rpx;
  font-size:15px;
  height:50rpx;
  line-height: 50rpx;
  color:#70aa52;
}
.body{
  background-color:#fff;
  padding:0 10rpx;
  border-radius:1px solid #ddd;
}
.body-icon__coupon{
  background-image:url(data:image/jpeg;base64,);
}
.body-icon__order{
  background-image: url("data:image/jpg;base64,");
}
.body-icon__address{
  background-image:url(data:image/jpeg;base64,);
}
.body{
  border-bottom:1px solid #ddd;
}
.body-item{
  text-align:left;
  height:115rpx;
  line-height: 115rpx;
  border-top: 1px solid #ddd;
}
.body-icon{
  width:45rpx;
  height:56rpx;
  margin:0 20rpx 0 40rpx;
  display:inline-block;
  background-position:center;
  background-size:cover;
  background-repeat: no-repeat;
  vertical-align:middle;
}
.body-icon-text{
  font-size:32rpx;
  width:400rpx;
  display: inline-block;
}
.body-arrow{
  margin-left:155rpx ;
  width:45rpx;
  height:56rpx;
  vertical-align:middle;
  background-position:right center;
  background-repeat:no-repeat;
  display: inline-block;
  background-image: url("data:image/jpg;base64,");
}