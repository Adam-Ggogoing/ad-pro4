.contaniner{
  width: 100%;
  position: relative;
  background: #8f8f8f;
  font-family: SimSun,Microsoft YaHei,self-Serif;
  overflow: scroll;
}
.swiper{
  width:100%;
  height:480rpx;
  background: #fff;
}
.swiper swiper-item{
  text-align: center
}
.swiper .slide-image{
  margin:0 auto;
  width:640rpx;
  height:480rpx;
}
.goods-info{
  background: #fff;
  padding:40rpx 34rpx 34rpx;
  position: relative;
}
.tag-content{
  color: #fff;
  background-color:#ff4400;
  padding:10rpx 24rpx; 
  font-size: 22rpx;
  text-align: center;
  border-radius:42rpx;
  margin-left:20rpx;
}
.tag-content-saleout{
  color: #fff;
  background-color:#c0c2bf;
  padding:10rpx 24rpx; 
  font-size: 22rpx;
  text-align: center;
  border-radius:42rpx;
  margin-left:20rpx;
}
.goods-title{
  font-size: 36rpx;
  line-height:52rpx;
  color:#424242;
  margin-bottom:36rpx;
}
.goods-subTitle{
  color: #8f8f8f;
  margin-bottom:46rpx;
  line-height:52rpx;
  font-size: 28rpx;
}
.goods-price{
  color:#ff4400;
  font-size: 40rpx;
  margin:0 30rpx 42rpx 0;
}
.goods-originPrice{
  font-size: 28rpx;
  color:#8f8f8f;
  text-decoration: line-through;
}
.goods-info-bottom{
  position:relative;
  background: #fff;
  height: 110rpx;
  line-height: 110rpx;
  padding:0 34rpx;
  border-top:1rpx solid #E6E6E6;
  font-size: 28rpx;
  border-bottom:1rpx solid #E6E6E6;
  text-align: center
}
.goods-info-bottom text{
  color: #8f8f8f
}
.goods-dispat{
  position: absolute;
  left:34rpx;
  top:0;
}
.goods-originplace{
  position: absolute;
  right:34rpx;
  top:0;
}
.goods-info-discount{
  box-sizing: border-box;
  width:100%;
  height:114rpx;
  background:#fff;
  font-size: 28rpx;
  color:#424242;
  padding:33rpx;
  margin-bottom:10rpx;
}
.discount-icon{
  display: inline-block;
  width:86rpx;
  height:48rpx;
  text-align: center;
  margin-right: 18rpx;
  line-height: 48rpx;
  border:1rpx solid #ff4400;
  color: #ff4400;
}
/*为你推荐*/
.recommand{
  height:102rpx;
  background-color:#fff;
  position: relative;
}
.recommand .icon{
  width:46rpx;
  height:50rpx;
  display: inline-block;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-position:center;
  background-size:100% ;
  position: absolute;
  top: 23rpx;
  left: 283rpx;
}
.recommand .recom-text{
  display: inline-block;
  font-size: 32rpx;
  color: #000;
  position: absolute;
  top: 30rpx;
  right: 283rpx;
}
.horizon-scroll{
  box-sizing:border-box;
  width:100%;
  white-space:nowrap;
  background:#fff;
  padding:0 20rpx 0 10rpx;
}
.hot-goods-content{
  box-sizing: border-box;
  width: 304rpx;
  height:514rpx;
  padding:30rpx;
  border:1rpx solid #E6E6E6;
  position: relative;
  margin: 0 10rpx 30rpx 20rpx  ;
  display: inline-block;
  vertical-align: top;
}
.itemPic{
  width:244rpx;
  height:244rpx;
  margin-bottom:15rpx;
}
.title{
  display: block;
  font-size: 28rpx;
  color:#424242;
  line-height: 43rpx;
  margin-bottom: 24rpx;
  white-space:normal;
}
.price{
  font-size: 28rpx;
  color: #ff4400;
  position: absolute;
  left: 30rpx;
  bottom: 30rpx;
}
.saleDetail{
  color: #8f8f8f;
}
.addCart{
  width: 66rpx;
  height:66rpx;
  background-image: url('http://h5.xianzaishi.com//upload_cb0e70b4983fa85bfc4db67fec960a67.png');
  background-size: 100%;
  position: absolute;
  right: 24rpx;
  bottom: 24rpx;
}
.addCart-saleout{
  background-image: url('http://h5.xianzaishi.com//upload_d6cafe95bd3fa60483cc22a0a1394f97.png');
}
/*加购*/
.buy-cart-wrap{
  width:100%;
  height:115rpx;
}
.buy-cart{
  width:100%;
  height: 115rpx;
  background-color: #f5f4f4;
  position: fixed;
  bottom:0;
  margin:0;
  padding:0;
}
.cart-icon{
  background-image: url("data:image/png;base64,");
  background-size:100%; 
  width:75rpx;
  height: 75rpx;
  display: inline-block;
  position: absolute;
  left: 40rpx;
  top: 20rpx;
}
.cart-tag{
  border-radius: 50%;
  display: inline-block;
  width:40rpx;
  height: 40rpx;
  background-color: #ff4400;
  color: #fff;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  left: 90rpx;
  top:10rpx;
}
.cart-text{
  display: inline-block;
  width:286rpx;
  height: 115rpx;
  line-height: 115rpx;
  text-align: center;
  font-size: 40rpx;
  color: #fff;
  background-color: #76b357;
  position: absolute;
  right: 0;
  top: 0;
}
.cart-text-saleout{
  background-color: #c0c2bf;
}