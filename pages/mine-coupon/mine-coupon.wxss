.container{
  overflow: scroll;
}
.empty{
  color:#78b25c;
  font-size: 32rpx;
  width: 100%;
  line-height: 800rpx;
  text-align: center;
}
.coup-tab{
   background-color:#1e1915;  
  position: fixed;
  width: 100%;
  z-index: 999
}
.tab-item{
  display: inline-block;
  width: 250rpx;
  height: 94rpx;
  line-height: 94rpx;
  text-align: center;
  color: #d2b47e;
  font-size: 30rpx;
}
.selected{
  background: #d2b47e;
  color:#1e1915;
}
.searchBox{
  margin:128rpx  34rpx 34rpx;
  position: relative;
}
.search-input{
  box-sizing: border-box;
  display: inline-block;
  padding-left:30rpx;
  width:534rpx;
  height:92rpx;
  background: #fff;
  border-radius:10rpx 0 0 10rpx;
}
.search-btn{
  display: inline-block;
  position: absolute;
  right:0;
  top:0;
  width:148rpx;
  height:92rpx;
  line-height: 92rpx;
  text-align: center;
  font-size: 30rpx;
  background-color: #ccc;
  color:#fff;
  border-radius:0 10rpx 10rpx 0;
}
.inputed{
  background-color:#7fb347; 
}
.coupon{
  position: relative;
  box-sizing: border-box;
  margin-bottom:34rpx;
}
.all-coupon-body{
  width:682rpx;
  height:204rpx;
  position:relative;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-position:center;
  background-size: 100%;
  margin:auto;
}
.limit-coupon-body{
  width:682rpx;
  height:204rpx;
  position:relative;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-position:center;
  background-size: 100%;
  margin:auto;
}
.other-coupon-body{
  width:682rpx;
  height:204rpx;
  position:relative;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-position:center;
  background-size: 100%;
  margin:auto;
}
.overdue-coupon-body{
  width:682rpx;
  height:204rpx;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-position:center;
  background-size: 100%;
  margin:auto;
}
.coupon-price{
  display: block;
  font-size:48rpx;
  color:#fff;
  padding:30rpx;
}
.coupon-expire{
  font-size: 28rpx;
  color: #fff;
  padding:0 30rpx;
}
.coupon-footer{
  width:682rpx;
  height:86rpx;
  background-image: url('http://h5.xianzaishi.com//upload_eab639872dd7274dfa09250dc05cf88a.png');
  background-repeat:no-repeat;
  background-position:center;
  background-size: 100%;
  margin:auto;
  text-align: center;
  line-height: 86rpx;
  font-size: 30rpx;
  color: #686868;
}
.footer{
  box-sizing: border-box;
  width:682rpx;
  margin:auto;
  line-height: 64rpx;
  font-size: 30rpx;
  color: #686868;
}
.desc-content{
  padding: 34rpx;
  background-color: #fff;
}
.close-text{
  line-height:68rpx;
}
.close{
  position: relative;
  width:682rpx;
  height:86rpx;
  background-image: url('http://h5.xianzaishi.com//upload_eab639872dd7274dfa09250dc05cf88a.png');
  background-size: 100%;
  display: inline-block;
  text-align: center;
  line-height: 86rpx;
  font-size: 30rpx;
  color: #686868;
  border-top:1rpx solid #E6E6E6;
}
.check-on{
  margin-left:-48rpx;
}
.check-off{
  margin-left: 276rpx;
}
.icon-on{
  position: absolute;
  top:28rpx;
  right:200rpx;
  background-image: url('http://h5.xianzaishi.com//upload_b7d48093b0cfca92a9c24687fb390676.png');
}
.icon-off{
  position: absolute;
  top:28rpx;
  right:276rpx;
  background-image: url('http://h5.xianzaishi.com//upload_eb5cb4b5eeda2d529701964b285d2fb8.png');
}
.close-icon{
  width:28rpx;
  height:28rpx;
  background-repeat:no-repeat;
  background-position:center;
  background-size: 100%;
  background-color:#1e1915; 
}
.footer-text{
  text-align: center;
}
