.container{
  background-color:#ececec;
  overflow: scroll;
}
.empty-img{
  width:339rpx;
  height:363rpx;
  background-image:url(http://h5.xianzaishi.com//upload_0ba2f7a33eb538dae405a3a9c23f0ac0.jpg);
  background-size:cover;
  margin:200rpx auto 100rpx;
}
.empty-goshop{
  width:320rpx;
  line-height:110rpx;
  text-decoration: none;
  display: block;
  margin:100rpx auto 0;
  border-radius:55rpx;
  background-color:#77b455;
  font-size:33rpx;
  color:#fff;
  text-align: center;
}
.topfixed{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background: #fffde2;
  width: 100%;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  font-size: 26rpx;
}
.topfixed-icon{
  color:#fff;
  background: #ff4400;
  display: inline-block;
  width: 116rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 20rpx; 
}
.cart-goods{
  background-color:#fff;
  height: 200rpx;
  padding:50rpx 0;
  margin-bottom:20rpx; 
  position: relative;
}
.cart-goods-icon{
  background-image: url("data:image/png;base64,");
  background-position:center;
  background-repeat: no-repeat;
  background-size: cover;
  width:63rpx;
  height:63rpx;
  position:absolute;
  left:30rpx;
  top:0;
  bottom:0;
  margin:auto 0;
  padding:0;
}
.cart-goods-icon-un{
  background-image: url("data:image/png;base64,");
}
.cart-goods-img{
  width:200rpx;
  height:230rpx;
  left:120rpx;
  position:absolute;
  top:50rpx;
}
.cart-goods-title{
  font-size:25rpx;
  color:#6c706f;
  padding-left:350rpx;
  line-height:64rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width:320rpx;
}
.cart-goods-spec{
  font-size:25rpx;
  color:#6c706f;
  padding-left:350rpx;
  line-height:64rpx;
  margin:50rpx 0;
}
.cart-goods-price{
  padding-left: 350rpx;
  line-height:64rpx;
}

.cart-goods-origin{
  font-size:25rpx;
  color:#ff4400;
}
.cart-goods-count{
  position:absolute;
  right:30rpx;
  bottom:50rpx;
  font-size:25rpx;
  vertical-align: middle;
}
.cart-goods-operation{
  vertical-align: middle;
  display: inline-block;
  width:66rpx;
  height:66rpx;
  line-height:66rpx;
  border-radius:50%;
  border:1px solid #e4e4e4;
  font-size:46rpx;
  text-align:center;
  color:#ff4400;
}
.cart-goods-num{
  margin:0 20rpx;
}
.cart-goods-del{
  position: absolute;
  width:35rpx;
  line-height:35rpx;
  text-align:center;
  background-color:#717171;
  border-radius:50%;
  color:#fff;
  font-size:25rpx;
  right:30rpx;
  top:40rpx;
  font-weight: bold;
  opacity:.5;
}
.cart-goods-tags{
  color: #fff;
  background-color:#ff4400;
  padding:10rpx 24rpx;  
  position: absolute;
  left:112rpx;
  top:52rpx;
  font-size: 22rpx;
  text-align: center;
  border-radius:42rpx;
}
/* 失效商品 */
.goods-invalid{
  background-color:#fff;
  height: 200rpx;
  padding:50rpx ;
  border-bottom: 1rpx solid #e4e4e4;
  position: relative;
}
.goods-invalid-container{
  position: relative;
  width: 100%;
  z-index: 0;
}
.modal{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  background: #fff;
  opacity: .6
}
.goods-invalid-text{
  text-align: center;
  font-size: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #e4e4e4;
}
.incalid-btn{
 margin:18rpx auto;
 width: 390rpx;
 height: 70rpx;
 font-size: 30rpx;
}
.cart-bottom-place{
  height: 190rpx;
}
.cart-bottom{
  height:115rpx;
  line-height:115rpx;
  background-color:#000;
  position:fixed;
  bottom:0;
  left:0;
  right:0;
  font-size:33rpx;
}
.cart-whole{
  width:51rpx;
  height:51rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  display:inline-block;
  vertical-align: middle;
  margin-left:30rpx;
}
.cart-whole-text{
  color:#fff;
  font-size:30rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
.cart-whole-unSelect{
  background-image: url("data:image/png;base64,");
}
.cart-total{
  color:#fff;
  font-size:40rpx;
  margin-left: 50rpx;
  vertical-align: middle;
}
.cart-account{
  background-color:#7fb347;
  position:absolute;
  right:0;
  height:115rpx;
  color:#fff;
  width:240rpx;
  text-align:center;
  line-height: 115rpx;
}
.cart-freight{
  line-height:75rpx;
  position:fixed;
  bottom:115rpx;
  left:0;
  right:0;
  margin:0;
  padding:0;
  background-color:#fff;
  text-align:center;
  color:#6c706f;
  border-top:1px solid #eaeaea;
  font-size:25rpx;
}
.cart-freight a{
  position:absolute;
  right:35rpx;
  text-decoration:none;
  color:#7fb347;
}

/* empty */
.cart-empty{
  padding-top:1px;
}
.cart-empty-pic{
  width:339rpx;
  height:363rpx;
  display:block;
  margin:220rpx auto 0;
}
.cart-empty-btn{
  width:320rpx;
  line-height:110rpx;
  text-decoration: none;
  display: block;
  margin:100rpx auto 0;
  border-radius:55rpx;
  background-color:#77b455;
  font-size:33rpx;
  color:#fff;
  text-align: center;
}
